{
  "name": "@mobilabs/kasar",
  "version": "0.0.1",
  "description": "View-powered Static Website Generator",
  "main": "index.js",
  "bin": {
    "kasar": "./bin/kasar.js"
  },
  "scripts": {
    "build": "gulp -f site/.kasar/gulpfile.js build",
    "app": "gulp -f site/.kasar/gulpfile.js serve",
    "bbbuild": "gulp build",
    "watch": "gulp watch",
    "makedist": "gulp makedist",
    "test": "cross-env NODE_ENV=test nyc --reporter=lcov mocha ./test/main.js",
    "display-coverage": "opener ./coverage/lcov-report/index.html",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage": "nyc report --reporter=text-lcov | coveralls",
    "report": "nyc report",
    "doc": "",
    "makeprivate": "gulp makeprivate && sh tasks/compress.sh ${npm_package_name} ${npm_package_version} @mobilabs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jclo/kasar.git"
  },
  "keywords": [],
  "author": {
    "name": "Mobilabs",
    "email": "contact@mobilabs.fr",
    "url": "http://www.mobilabs.fr"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jclo/kasar/issues"
  },
  "homepage": "https://github.com/jclo/kasar",
  "dependencies": {
    "@mobilabs/view": "0.0.2",
    "browser-sync": "2.26.7",
    "cross-env": "6.0.3",
    "del": "5.1.0",
    "font-awesome": "4.7.0",
    "fs-extra": "8.1.0",
    "gulp": "4.0.2",
    "gulp-clean-css": "4.2.0",
    "gulp-concat": "2.6.1",
    "gulp-footer": "2.0.2",
    "gulp-header": "2.0.9",
    "gulp-rename": "1.4.0",
    "gulp-replace": "1.0.0",
    "gulp-uglify-es": "2.0.0",
    "html5-boilerplate": "7.3.0",
    "jquery": "3.4.1",
    "jquery.easing": "1.4.1",
    "jsdom": "15.2.1",
    "markdown-it": "10.0.0",
    "markdown-it-anchor": "5.2.5",
    "markdown-it-attrs": "3.0.1",
    "markdown-it-container": "2.0.0",
    "nopt": "4.0.1",
    "path": "0.12.7",
    "purecss": "1.0.1",
    "shelljs": "0.8.3"
  },
  "devDependencies": {
    "eslint": "6.6.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-import": "2.18.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "private": false,
  "husky": {
    "hooks": {
      "pre-commit": "npm run build && npm run makedist && npm run test && npm run check-coverage && npm run report"
    }
  }
}
